{
  "private": true,
  "name": "@doars/workspace",
  "workspaces": [
    ".docs/",
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "build": "yarn workspaces run build",
    "docs:build": "yarn workspace @doars/docs run build",
    "docs:develop": "yarn workspace @doars/docs run develop",
    "lint": "yarn workspaces run lint",
    "test": "yarn workspaces run test",
    "test:browser": "yarn workspaces run test:browser",
    "test:runtime": "yarn workspaces run test:runtime"
  },
  "devDependencies": {
    "@babel/core": "7.19.x",
    "@babel/preset-env": "7.19.x",
    "@playwright/test": "1.25.x",
    "brotli-size": "4.0.x",
    "chai": "4.3.x",
    "esbuild": "0.15.x",
    "esbuild-plugin-babel": "0.2.x",
    "eslint": "8.23.x",
    "eslint-config-standard": "17.0.x",
    "eslint-plugin-import": "2.26.x",
    "eslint-plugin-n": "15.0.x",
    "eslint-plugin-node": "11.1.x",
    "eslint-plugin-promise": "6.0.x",
    "eslint-plugin-standard": "5.0.x",
    "mocha": "10.0.x"
  }
}
